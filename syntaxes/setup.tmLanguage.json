{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Setup",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.setup",
          "match": "\\b(create|insert|into)\\b"
        },
        {
          "name": "keyword.operator.setup",
          "match": "(>)"
        }
      ]
    },
    "strings": {
      "name": "string.quoted.single.setup",
      "begin": "'",
      "end": "'",
      "patterns": [
        {
          "name": "constant.character.escape.setup",
          "match": "\\\\."
        }
      ]
    }
  },
  "scopeName": "source.setup"
}